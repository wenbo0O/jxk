{
  "name": "jxk",
  "type": "module",
  "version": "0.0.5",
  "private": false,
  "description": "A relatively commonly used tool and function",
  "author": "xkloveme <xkloveme@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/xkloveme/jxk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xkloveme/jxk.git"
  },
  "bugs": "https://github.com/xkloveme/jxk/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "jxk",
    "git",
    "project",
    "version"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "node generate.js",
    "docs": "rimraf docs & jsdoc -c jsdoc.json",
    "dev": "npm run gen && npm run play",
    "build": "npm run gen && unbuild",
    "lint": "eslint .",
    "play": "vite playground",
    "play:install": "pnpm -r --filter=playground install",
    "play:build": "pnpm -r --filter=playground run build-preview",
    "release": "npm run build && bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.1",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.2.0",
    "bumpp": "^9.5.1",
    "docdash-extended-xkloveme": "^1.0.2",
    "eslint": "^9.9.0",
    "esno": "^4.7.0",
    "jsdoc": "^4.0.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.0",
    "vite-plugin-git-version": "^1.0.7",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "date-fns": "^3.6.0"
  }
}
